<AnchorPane id="AnchorPane" prefHeight="750.0" prefWidth="1000.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Davenrun.Venus_wellController">
    <children>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="750.0" prefWidth="300.0">
            <children>
                <TextArea fx:id = "current" layoutY = "0" text="Quest "/>
                <TextArea fx:id = "Map" layoutY="350" layoutX = "0" text = "map"/> 
                
            </children>
        </AnchorPane>
        <AnchorPane layoutX="300.0" prefHeight="511.0" prefWidth="700.0">
            <children>
                <Pane prefHeight="523.0" prefWidth="660.0">
                    <children>
                        <TextArea fx:id="prompt" layoutX="0" layoutY="0.0"  prefHeight="75.0"/>
    
                        <ComboBox fx:id="Actions" layoutX="425.0" layoutY="200.0" prefWidth="150.0" /> 
                        <Button fx:id="button" layoutX="425" layoutY="250.0" mnemonicParsing="false" text="Submit Action" />
                        <Button fx:id="button2" layoutX="200" layoutY="250.0" mnemonicParsing="false" text="Submit Action" />
