                        <Button fx:id = "Go_East" layoutX="300" layoutY="100.0" mnemonicParsing="false" text="Go East" onAction = "#Go_East" /> 
                      
                        <Button fx:id="Go_SouthEast" layoutX="400" layoutY="100.0" mnemonicParsing="false" text="Go SouthEast"  onAction = "#Go_SouthEast"/>       
                      
                        <Button fx:id = "Player_stats" layoutX="325" layoutY="200.0"  mnemonicParsing="false" text="Player stats"  onAction = "#stats" />  
                        <TextArea fx:id="stats" layoutX="100" layoutY="300.0"/>  
                        <ComboBox fx:id = "Look" layoutX="200" layoutY="200.0" />             
                    </children>
                </Pane>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
