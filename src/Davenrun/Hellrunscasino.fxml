<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="750.0" prefWidth="1000.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Davenrun.HellrunscasinoController">
    <children>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="750.0" prefWidth="300.0">
            <children>
                <TextArea fx:id = "current" layoutY = "0" text="Quest "/>
                <TextArea fx:id = "Map" layoutY="350" layoutX = "0" text = "map"/> 
                
            </children>
        </AnchorPane>
        <AnchorPane layoutX="300.0" prefHeight="511.0" prefWidth="700.0">
            <children>
                <Pane prefHeight="523.0" prefWidth="660.0">
                    <children>
                       
                        
                        <Button fx:id="button" layoutX="200" layoutY="250.0" mnemonicParsing="false" text="Submit Action" />
                        
                        <TextArea fx:id="prompt" layoutX="100" layoutY="0.0" /> 
                        <Button fx:id="Go_inside" layoutX="100" layoutY="100.0" mnemonicParsing="false" text="Go inside Casino" onAction = "#Go_inside" />
                        <Button fx:id="Go_North" layoutX="250" layoutY="100.0" mnemonicParsing="false" text="Go North" onAction = "#go_north" />       
                        <Button fx:id = "Go_East" layoutX="350" layoutY="100.0" mnemonicParsing="false" text="Go East" onAction = "#go_east"/> 
                        <Button fx:id = "Sean_Militen" layoutX="50" layoutY="200" mnemonicParsing="false" text= "Talk to Sean Militen" onAction="#Sean_Militen"/> 
                        <ComboBox fx:id = "Look" layoutX="200" layoutY="200.0" />         
                        <Button fx:id = "Player_stats" layoutX="350" layoutY="200.0"  mnemonicParsing="false" text="Player stats"  onAction = "#stats" />  
                        <TextArea fx:id="stats" layoutX="100" layoutY="300.0"/>  
                    </children>
                </Pane>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>

