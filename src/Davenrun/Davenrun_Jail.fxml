<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Davenrun.Davenrun_JailController">
   <children>

        <TextArea fx:id="prompt" layoutX="100" layoutY="0.0" text = "Welcome to the Davenrun Jail" />
        <Button fx:id="Davenrun_Guard" layoutX="100" layoutY="100.0" mnemonicParsing="false" text="Talk to Davenrun Guard" onAction = "#Davenrun_Guard" />
        <Button fx:id="Prisoner1" layoutX="250" layoutY="100.0" mnemonicParsing="false" text="Talk to Prisoner"  onAction = "#prisoner1" />
        <Button fx:id="Prisoner2" layoutX="400" layoutY="100.0" mnemonicParsing="false" text="Talk to Prisoner" onAction = "#prisoner2" />
        <Button fx:id="Prisoner3" layoutX="100" layoutY="200.0" mnemonicParsing="false" text="Talk to Prisoner" onAction = "#prisoner3" />
 
        <Button fx:id="Jarls_office" layoutX="250" layoutY="200.0" mnemonicParsing="false" text="Go to jarls office" onAction="#jarls_office" />
        <Button fx:id="Guard_Center" layoutX="400" layoutY="200.0" mnemonicParsing="false" text="Go to guard center" onAction ="#gaurd_Center"/>
        
      
        <Button fx:id="Exit" layoutX="100" layoutY="300.0" mnemonicParsing="false" text="Exit"  onAction = "#exit"/>
    </children>
</AnchorPane>
