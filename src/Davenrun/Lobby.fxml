<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Davenrun.LobbyController">
    <children>
        
        <TextArea fx:id="prompt" layoutX="100" layoutY="0.0" text = "Welcome to the lobby" />
        <Button fx:id="Game_Room" layoutX="400" layoutY="100.0" mnemonicParsing="false" text="Go to gameroom"  onAction="#game_Room" />
        <Button fx:id="Management_center" layoutX="250" layoutY="100.0" mnemonicParsing="false" text="Go to management center" onAction = "#Management_center"/>
        <ComboBox fx:id="Actions" layoutX="100.0" layoutY="100.0"  prefWidth="150.0" /> 
        <Button fx:id="button" layoutX="100" layoutY="200.0" mnemonicParsing="false" text="Submit Action" onAction="#doAction" />
      
        <Button fx:id="Exit" layoutX="200" layoutY="200.0" mnemonicParsing="false" text="Leave lobby" onAction = "#exit" />
    </children>
</AnchorPane>
