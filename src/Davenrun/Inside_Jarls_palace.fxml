<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Davenrun.Inside_Jarls_palaceController">
    <children>
      <TextArea fx:id="prompt" layoutX="100" layoutY="0.0" text="Welcome to Jarls Palace!"  />
        <Button fx:id="game_room" layoutX="100" layoutY="100.0" mnemonicParsing="false" text="Go to Jarls office"  onAction = "#game_Room" />
        <Button fx:id="lobby" layoutX="250" layoutY="100.0" mnemonicParsing="false" text="Go to guard center." onAction="#lobby" />
        <Button fx:id="mangement_center" layoutX="400" layoutY="100.0" mnemonicParsing="false" text="Go to Jail" onAction = "#Management_center" />       
        <Button fx:id="Exit" layoutX="550" layoutY="100.0" mnemonicParsing="false" text="Exit"  onAction = "#exit"/>
     </children>  
</AnchorPane>
