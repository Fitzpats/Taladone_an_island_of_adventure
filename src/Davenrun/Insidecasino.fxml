<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Davenrun.InsidecasinoController">
    <children>
        <TextArea fx:id="prompt" layoutX="100" layoutY="0.0" text="Aye, welcome to Hell runs casino. Enjoy the drinks and games traveler."  />
        <Button fx:id="game_room" layoutX="100" layoutY="100.0" mnemonicParsing="false" text="Go to game room."  onAction = "#game_Room" />
        <Button fx:id="lobby" layoutX="250" layoutY="100.0" mnemonicParsing="false" text="Go to lobby." onAction="#lobby" />
        <Button fx:id="mangement_center" layoutX="100" layoutY="200.0" mnemonicParsing="false" text="Go to management center." onAction = "#Management_center" />       
        <Button fx:id="Exit" layoutX="275" layoutY="200.0" mnemonicParsing="false" text="Exit" onAction = "#exit"/>
    </children>  
</AnchorPane>
